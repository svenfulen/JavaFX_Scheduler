<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main_package.customers_table_controller">
   <children>
      <TableView fx:id="customers_table" layoutX="11.0" layoutY="14.0" onSort="#populateTable" prefHeight="310.0" prefWidth="575.0">
        <columns>
          <TableColumn fx:id="id_column" prefWidth="26.0" text="ID" />
          <TableColumn fx:id="name_column" prefWidth="142.0" text="Name" />
            <TableColumn fx:id="phone_column" prefWidth="128.0" text="Phone" />
            <TableColumn fx:id="address_column" prefWidth="135.0" text="Address" />
            <TableColumn fx:id="zip_column" prefWidth="67.0" text="Zip Code" />
            <TableColumn fx:id="division_column" prefWidth="74.0" text="Division" />
        </columns>
      </TableView>
      <Button fx:id="remove_button" layoutX="141.0" layoutY="343.0" mnemonicParsing="false" onAction="#remove_customer" prefHeight="25.0" prefWidth="62.0" text="Remove" />
      <Button fx:id="add_button" layoutX="11.0" layoutY="343.0" mnemonicParsing="false" onAction="#open_addCustomerForm" prefHeight="25.0" prefWidth="52.0" text="Add" />
      <Button fx:id="modify_button" layoutX="71.0" layoutY="343.0" mnemonicParsing="false" onAction="#open_modifyCustomerForm" prefHeight="25.0" prefWidth="62.0" text="Modify" />
   </children>
</AnchorPane>
